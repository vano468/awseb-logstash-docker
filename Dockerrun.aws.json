{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "logs",
      "image": "<replace-with-container-name>",
      "essential": true,
      "memory": 400,
      "portMappings": [
        {
          "hostPort": 9900,
          "containerPort": 9900,
          "protocol": "udp"
        }, {
          "hostPort": 80,
          "containerPort": 80
        }, {
          "hostPort": 9901,
          "containerPort": 9901
        }
      ]
    }
  ]
}
